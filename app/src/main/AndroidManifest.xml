<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CollapsingToolbar"
            android:parentActivityName=".ProductMain" />
        <activity
            android:name=".ProductMain"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".pop_up_product_manipulate" />
        <activity android:name=".product_update"></activity>
        <activity android:name=".OrderMain"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".OrderView"
            android:parentActivityName=".OrderMain"/>
    </application>

</manifest>
